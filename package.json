{
  "name": "utmap-times",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "npm-run-all graphql:codegen lint compile",
    "compile": "next build",
    "graphql:codegen": "apollo client:codegen --target typescript --outputFlat",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "autoprefixer": "^10.2.4",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.4",
    "next": "^10.0.6",
    "postcss": "^8.2.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "apollo": "^2.32.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint"
    ]
  }
}
