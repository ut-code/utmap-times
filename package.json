{
  "name": "utmap-times",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "prebuild": "npm-run-all graphql:codegen prebuild-script lint",
    "build": "next build",
    "graphql:codegen": "apollo client:codegen --target typescript --outputFlat",
    "graphql:codegen:watch": "apollo client:codegen --watch --target typescript --outputFlat",
    "prebuild-script": "ts-node --project scripts/tsconfig.json scripts/prebuild",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "autoprefixer": "^10.2.4",
    "clsx": "^1.1.1",
    "cross-fetch": "^3.0.6",
    "dayjs": "^1.10.4",
    "marked": "^2.0.0",
    "next": "^10.0.6",
    "nprogress": "^0.2.0",
    "postcss": "^8.2.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "sanitize-html": "^2.3.2",
    "styled-components": "^5.2.1",
    "superstruct": "^0.14.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.4",
    "@types/marked": "^1.2.2",
    "@types/node": "^14.14.22",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/sanitize-html": "^1.27.1",
    "@types/styled-components": "^5.1.7",
    "apollo": "^2.32.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript-prettier": "^4.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint"
    ]
  }
}
