{
  "name": "utmap-times",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "prebuild": "npm-run-all graphql:codegen prebuild-script lint",
    "build": "next build",
    "graphql:codegen": "apollo client:codegen --target typescript --outputFlat",
    "graphql:codegen:watch": "apollo client:codegen --watch --target typescript --outputFlat",
    "prebuild-script": "ts-node --project scripts/tsconfig.json scripts/prebuild",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc"
  },
  "dependencies": {
    "@apollo/client": "^3.4.13",
    "@rehooks/local-storage": "^2.4.3",
    "autoprefixer": "^10.4.0",
    "clsx": "^1.1.1",
    "cross-fetch": "^3.1.4",
    "datocms-structured-text-utils": "^1.2.0",
    "dayjs": "^1.10.7",
    "markdown-to-jsx": "^7.1.5",
    "next": "^12.0.7",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.4",
    "react": "^17.0.2",
    "react-datocms": "^1.6.6",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-intersection-observer": "^8.33.0",
    "styled-components": "^5.3.3",
    "superstruct": "^0.15.3",
    "tailwindcss": "^3.0.1",
    "use-local-storage-state": "^13.0.0",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@types/gtag.js": "^0.0.8",
    "@types/marked": "^4.0.1",
    "@types/node": "^16.11.12",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/sanitize-html": "^2.6.0",
    "@types/styled-components": "^5.1.17",
    "apollo": "^2.33.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "eslint-config-next": "^11.1.2",
    "husky": "^4.3.8",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint"
    ]
  }
}
